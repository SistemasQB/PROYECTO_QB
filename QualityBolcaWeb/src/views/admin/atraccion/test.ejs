/* Instructions Styles */
.instructions {
    display: flex;
    align-items: flex-start;
    gap: 1.5rem;
    background-color: var(--light);
    padding: 1.5rem;
    border-radius: var(--radius-sm);
    border-left: 4px solid var(--secondary);
}

.instruction-icon {
    color: var(--primary);
    flex-shrink: 0;
}

/* Traits Styles */
.traits-container {
    border: 1px solid var(--gray-light);
    border-radius: var(--radius-sm);
    overflow: hidden;
}

.traits-header {
    display: flex;
    background-color: var(--primary);
    color: white;
    padding: 1rem;
}

.trait-name-header {
    flex: 1;
    font-weight: 600;
}

.trait-options-header {
    display: flex;
    width: 120px;
}

.trait-options-header span {
    flex: 1;
    text-align: center;
    font-weight: 600;
}

.traits-group {
    border-bottom: 1px solid var(--gray-light);
}

.traits-group:last-child {
    border-bottom: none;
}

.group-title {
    background-color: var(--gray-light);
    padding: 0.75rem 1rem;
    font-weight: 600;
    color: var(--gray-dark);
}

.trait-item {
    display: flex;
    align-items: center;
    padding: 0.75rem 1rem;
    border-bottom: 1px solid var(--gray-light);
}

.trait-item:last-child {
    border-bottom: none;
}

.trait-name {
    flex: 1;
    font-weight: 500;
}

.trait-options {
    display: flex;
    width: 120px;
}

.trait-options .radio-label {
    flex: 1;
    display: flex;
    justify-content: center;
}

/* Form Actions */
.form-actions {
    display: flex;
    justify-content: center;
    margin-top: 2rem;
    margin-bottom: 2rem;
}

.btn-submit {
    padding: 0.75rem 2.5rem;
    border: none;
    border-radius: var(--radius-sm);
    font-family: 'Nunito', sans-serif;
    font-weight: 600;
    font-size: 1.1rem;
    cursor: pointer;
    transition: var(--transition);
    background-color: var(--primary);
    color: white;
    box-shadow: var(--shadow);
}

.btn-submit:hover {
    background-color: var(--primary-dark);
    transform: translateY(-2px);
    box-shadow: var(--shadow-lg);
}

/* Footer */
footer {
    text-align: center;
    color: var(--gray-dark);
    padding: 1rem 0;
    border-top: 1px solid var(--gray-light);
}

/* Responsive Design */
@media (max-width: 768px) {
    .container {
        padding: 1rem;
    }
    
    .form-row {
        flex-direction: column;
        gap: 1rem;
    }
    
    .form-section {
        padding: 1.5rem;
    }
    
    .trait-options {
        width: 100px;
    }
}
</style>

