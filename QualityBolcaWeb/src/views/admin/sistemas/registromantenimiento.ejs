    // Función para buscar registros
    function buscarRegistros(termino) {
        if (!termino) {
            cargarRegistros();
            return;
        }

        termino = termino.toLowerCase();
        const resultados = registros.filter(registro =>
            registro.id.toLowerCase().includes(termino) ||
            registro.usuario.toLowerCase().includes(termino)
        );

        cargarRegistros(resultados);
    }

    // Inicializar la página
    document.addEventListener('DOMContentLoaded', function () {
        // Cargar los registros iniciales
        cargarRegistros();

        // Configurar el botón de búsqueda
        document.getElementById('searchButton').addEventListener('click', function () {
            const termino = document.getElementById('searchInput').value;
            buscarRegistros(termino);
        });

        // Permitir búsqueda al presionar Enter
        document.getElementById('searchInput').addEventListener('keypress', function (e) {
            if (e.key === 'Enter') {
                const termino = this.value;
                buscarRegistros(termino);
            }
        });

        // Configurar el cierre del modal
        document.querySelector('.close-button').addEventListener('click', function () {
            document.getElementById('recordModal').style.display = 'none';
        });

        // Cerrar el modal al hacer clic fuera del contenido
        window.addEventListener('click', function (event) {
            const modal = document.getElementById('recordModal');
            if (event.target === modal) {
                modal.style.display = 'none';
            }
        });
    });
