    <style>
        :root {
            --bg-color: #f8fafc;
            --text-primary: #1f2937;
            --text-secondary: #6b7280;
            --border-color: #e5e7eb;
            --hover-bg: #f1f5f9;
            --folder-color: #3b82f6;
            --file-color: #6b7280;
            --line-color: #cbd5e1;
            --animation-speed: 0.3s;
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: system-ui, -apple-system, sans-serif;
        }

        body {
            background-color: var(--bg-color);
            color: var(--text-primary);
            line-height: 1.5;
            padding: 2rem;
        }

        .tree-container {
            max-width: 800px;
            margin: 0 auto;
            background: white;
            padding: 2rem;
            border-radius: 1rem;
            box-shadow: 0 4px 6px -1px rgb(0 0 0 / 0.1);
        }

        .tree {
            list-style: none;
        }

        .tree-item {
            position: relative;
        }

        .tree-content {
            display: flex;
            align-items: center;
            padding: 0.75rem;
            cursor: pointer;
            border-radius: 0.5rem;
            transition: background-color var(--animation-speed);
            margin: 0.25rem 0;
        }

        .tree-content:hover {
            background-color: var(--hover-bg);
        }

        .tree-toggle {
            width: 24px;
            height: 24px;
            display: flex;
            align-items: center;
            justify-content: center;
            margin-right: 0.5rem;
            color: var(--text-secondary);
            transition: transform var(--animation-speed);
        }

        .tree-toggle.expanded {
            transform: rotate(90deg);
        }

        .tree-icon {
            width: 24px;
            height: 24px;
            display: flex;
            align-items: center;
            justify-content: center;
            margin-right: 0.75rem;
        }

        .tree-icon.folder {
            color: var(--folder-color);
        }

        .tree-icon.file {
            color: var(--file-color);
        }

        .tree-label {
            font-size: 0.95rem;
            color: var(--text-primary);
        }

        .tree-children {
            margin-left: 2.5rem;
            position: relative;
            overflow: hidden;
            transition: all var(--animation-speed);
        }

        /* Líneas conectoras */
        .tree-children::before {
            content: '';
            position: absolute;
            left: -17px;
            top: 0;
            bottom: 0;
            width: 2px;
            background-color: var(--line-color);
        }

        .tree-item::before {
            content: '';
            position: absolute;
            left: -17px;
            top: 24px;
            width: 20px;
            height: 2px;
            background-color: var(--line-color);
        }

        /* Animación para colapsar/expandir */
        .tree-children.collapsed {
            height: 0;
            opacity: 0;
        }

        .tree-children.expanded {
            height: auto;
            opacity: 1;
        }

        /* Estilos para diferentes tipos de archivos */
        .file-type {
            font-size: 0.75rem;
            padding: 0.25rem 0.5rem;
            border-radius: 0.25rem;
            margin-left: 0.75rem;
            background-color: var(--hover-bg);
            color: var(--text-secondary);
        }

        /* Contador de elementos */
        .item-count {
            font-size: 0.75rem;
            color: var(--text-secondary);
            margin-left: auto;
            padding: 0.25rem 0.5rem;
            background-color: var(--hover-bg);
            border-radius: 0.25rem;
        }
    </style>
</head>
